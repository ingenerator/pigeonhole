{
  "name":        "ingenerator/pigeonhole",
  "description": "Flash message management for kohana",
  "keywords":    ["kohana"],
  "homepage":    "https://github.com/ingenerator/pigeonhole",
  "type":        "kohana-module",
  "license":     "BSD-3-Clause",
  "authors":     [
    {
      "name":     "Andrew Coulton",
      "email":    "andrew@ingenerator.com",
      "homepage": "http://www.ingenerator.com"
    }
  ],
  "require": {
    "php":                     "^7.4",
    "ingenerator/kohana-core": "^4.7"
  },
  "require-dev": {
    "kohana/koharness": "dev-master",
    "phpspec/phpspec":  "^6.1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://php-packages.ingenerator.com"
    }
  ],
  "suggest": {
    "zeelot/kohana-dependencies": "Service container for Kohana"
  },
  "support": {
    "issues": "https://github.com/ingenerator/pigeonhole/issues",
    "source": "https://github.com/ingenerator/pigeonhole"
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\": "spec"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
